import java.io.File;
import java.io.FileInputStream;
import java.io.IOException;
import java.io.ObjectInputStream;
import java.util.ArrayList;

public class Bank {
	
	private ArrayList<BankAccount> bankAccounts;
	private FileInputStream inn;
	private ObjectInputStream in;
	
	public void openFile(String fileName) throws IOException, ClassNotFoundException{
		inn = new FileInputStream(new File(fileName));
		in = new ObjectInputStream(inn);
		bankAccounts = (ArrayList<BankAccount>)in.readObject();
	}
}
